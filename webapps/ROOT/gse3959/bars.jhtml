<%
for (int j=0; j<samples.length; j++) {
				   String bg = "";
				   if (samples[j].condition.equals("WT")) bg = "bg0";
				   if (samples[j].condition.equals("LEC2GR")) bg = "bg2";
				   long height = 0;
				   if (expr.values[j]>0) height = Math.round(Math.log(expr.values[j]*10)*10);
	%>
	<td><img class="<%=bg%>" src="/images/spacer.gif" width="10" height="<%=height%>"/></td>
	<%
	} // samples loop
	%>
